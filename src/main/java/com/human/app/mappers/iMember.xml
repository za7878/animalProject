<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.human.app.web.Member.DAO.MemberDAO">

<insert id="insertMember"> insert into member(name,loginid,passcode)values(#{param1},#{param2},#{param3}) </insert>

<select id="selectMemberCount" resultType="int"> select count(*) from member where loginid=#{param1} and passcode=#{param2} </select>

</mapper>